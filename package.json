{
  "name": "dictionary-web-app",
  "version": "0.1.0",
  "homepage": "https://kwln.github.io/frontend-mentor-dictionary-web-app",
  "private": true,
  "dependencies": {
    "@types/node": "^16.18.50",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.5",
    "react-svg": "^16.1.27",
    "styled-components": "^6.0.7",
    "typescript": "^4.9.5",
    "use-sound": "^4.0.1",
    "usehooks-ts": "^2.9.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*{.js,.jsx,.ts,.tsx,.html,.md,.json}": "npm run precommit"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.html,.md,.json src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx,.html,.md,.json --fix src",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,html,md,json}' --config .prettierrc",
    "precommit": "npm run lint:fix && npm run format",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.4.1",
    "@storybook/addon-interactions": "^7.4.1",
    "@storybook/addon-links": "^7.4.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.1",
    "@storybook/preset-create-react-app": "^7.4.1",
    "@storybook/react": "^7.4.1",
    "@storybook/react-webpack5": "^7.4.1",
    "@storybook/testing-library": "^0.2.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "storybook": "^7.4.1",
    "webpack": "^5.88.2"
  }
}
